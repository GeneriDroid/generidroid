From 11a17726b6ccbef17660bff204dfe5f858250662 Mon Sep 17 00:00:00 2001
From: Alberto Ponces <ponces26@gmail.com>
Date: Thu, 16 Nov 2023 00:24:55 +0000
Subject: [PATCH] fix: Build ParanoidSense on arm32 devices too

---
 config/common.mk | 4 ----
 1 file changed, 4 deletions(-)

diff --git a/config/common.mk b/config/common.mk
index c3b1da5..4aae9b9 100644
--- a/config/common.mk
+++ b/config/common.mk
@@ -13,9 +13,6 @@ PRODUCT_COPY_FILES += \
     frameworks/native/data/etc/android.software.sip.voip.xml:$(TARGET_COPY_OUT_PRODUCT)/etc/permissions/android.software.sip.voip.xml
 
 # Face Unlock
-TARGET_FACE_UNLOCK_SUPPORTED ?= $(TARGET_SUPPORTS_64_BIT_APPS)
-
-ifeq ($(TARGET_FACE_UNLOCK_SUPPORTED),true)
 PRODUCT_PACKAGES += \
     ParanoidSense
 
@@ -24,7 +21,6 @@ PRODUCT_SYSTEM_EXT_PROPERTIES += \
 
 PRODUCT_COPY_FILES += \
     frameworks/native/data/etc/android.hardware.biometrics.face.xml:$(TARGET_COPY_OUT_SYSTEM)/etc/permissions/android.hardware.biometrics.face.xml
-endif
 
 # Enforce privapp-permissions whitelist
 PRODUCT_SYSTEM_PROPERTIES += \
-- 
2.34.1

